   <text:p text:style-name="P42"><text:span text:style-name="Source_20_Text"><text:span text:style-name="T61"><text:s text:c="16"/></text:span></text:span><text:span text:style-name="Source_20_Text"><text:span text:style-name="T62">max_len </text:span></text:span><text:span text:style-name="Source_20_Text"><text:span text:style-name="T67">=</text:span></text:span><text:span text:style-name="Source_20_Text"><text:span text:style-name="T62"> i</text:span></text:span><text:span text:style-name="Source_20_Text"><text:span text:style-name="T67">+</text:span></text:span><text:span text:style-name="Source_20_Text"><text:span text:style-name="T68">1</text:span></text:span></text:p>
   <text:p text:style-name="P42"><text:span text:style-name="Source_20_Text"><text:span text:style-name="T61"><text:s text:c="12"/></text:span></text:span><text:span text:style-name="Source_20_Text"><text:span text:style-name="T63"># NOTE: &apos;in&apos; operation in dictionary to search </text:span></text:span></text:p>
   <text:p text:style-name="P42"><text:span text:style-name="Source_20_Text"><text:span text:style-name="T61"><text:s text:c="20"/></text:span></text:span><text:span text:style-name="Source_20_Text"><text:span text:style-name="T63"># key takes O(1). Look if current sum is seen </text:span></text:span></text:p>
