   <text:p text:style-name="P42"><text:span text:style-name="Source_20_Text"><text:span text:style-name="T61"><text:s text:c="16"/></text:span></text:span><text:span text:style-name="Source_20_Text"><text:span text:style-name="T62">max_len </text:span></text:span><text:span text:style-name="Source_20_Text"><text:span text:style-name="T67">=</text:span></text:span><text:span text:style-name="Source_20_Text"><text:span text:style-name="T62"> </text:span></text:span><text:span text:style-name="Source_20_Text"><text:span text:style-name="T68">1</text:span></text:span></text:p>
