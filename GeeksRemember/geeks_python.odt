   <text:p text:style-name="P40"><text:span text:style-name="T20">   </text:span><text:span text:style-name="Strong_20_Emphasis"><text:span text:style-name="T18">for i=7,  a[7] =  -3</text:span></text:span><text:span text:style-name="T23"><text:line-break/>   max_ending_here = max_ending_here + (-3)<text:line-break/>   max_ending_here = 4</text:span></text:p>
